<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceScreen
        android:key="key_fw_cfg_download"
        android:title="@string/title_fw_cfg_download" >
        <intent
            android:targetClass="com.goodix.gftest.DownloadFwCfgTestActivity"
            android:targetPackage="com.goodix.gftest" />
    </PreferenceScreen>

    <PreferenceCategory
        android:key="feature"
        android:layout="@layout/preference_category"
        android:title="@string/feature_category" >
        <ListPreference
            android:key="key_max_fingers"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_max_fingers" />
        <ListPreference
            android:key="key_max_fingers_per_user"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_max_fingers_per_user" />

        <SwitchPreference
            android:key="key_support_key_mode"
            android:summary="@string/summary_support_key_mode"
            android:title="@string/title_support_key_mode" />
        <SwitchPreference
            android:key="key_support_ff_mode"
            android:summary="@string/summary_support_ff_mode"
            android:title="@string/title_support_ff_mode" />
        <SwitchPreference
            android:dependency="key_support_ff_mode"
            android:key="key_support_power_key_feature"
            android:summary="@string/summary_support_power_key_feature"
            android:title="@string/title_support_power_key_feature" />
        <SwitchPreference
            android:key="key_forbidden_untrusted_enroll"
            android:summary="@string/summary_forbidden_untrusted_enroll"
            android:title="@string/title_forbidden_untrusted_enroll" />
        <SwitchPreference
            android:key="key_forbidden_enroll_duplicate_fingers"
            android:summary="@string/summary_forbidden_enroll_duplicate_fingers"
            android:title="@string/title_forbidden_enroll_duplicate_fingers" />
        <SwitchPreference
            android:key="key_support_bio_assay"
            android:title="@string/title_support_bio_assay" />
        <SwitchPreference
            android:key="key_support_performance_dump"
            android:summary="@string/summary_support_performance_dump"
            android:title="@string/title_support_performance_dump" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="nav"
        android:layout="@layout/preference_category"
        android:title="@string/nav_category" >

        <ListPreference
            android:entries="@array/support_nav_mode_entries"
            android:entryValues="@array/support_nav_mode_entry_values"
            android:key="key_support_nav_mode"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_support_nav_mode" />

        <ListPreference
            android:entries="@array/nav_double_click_time_entries"
            android:entryValues="@array/nav_double_click_time_entry_values"
            android:key="key_nav_double_click_time"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_nav_double_click_time" />

        <ListPreference
            android:entries="@array/nav_long_press_time_entries"
            android:entryValues="@array/nav_long_press_time_entry_values"
            android:key="key_nav_long_press_time"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_nav_long_press_time" />

    </PreferenceCategory>

    <PreferenceCategory
        android:key="enroll"
        android:layout="@layout/preference_category"
        android:title="@string/enroll_category" >
        <ListPreference
            android:key="key_enrolling_min_templates"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_enrolling_min_templates" />
        <ListPreference
            android:entries="@array/array_max"
            android:entryValues="@array/array_max"
            android:key="key_valid_image_quality_threshold"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_valid_image_quality_threshold" />
        <ListPreference
            android:entries="@array/array_max"
            android:entryValues="@array/array_max"
            android:key="key_valid_image_area_threshold"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_valid_image_area_threshold" />
        <ListPreference
            android:key="key_duplicate_finger_overlay_score"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_duplicate_finger_overlay_score" />
        <ListPreference
            android:key="key_increase_rate_between_stitch_info"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_increase_rate_between_stitch_info" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="authenticate"
        android:layout="@layout/preference_category"
        android:title="@string/authenticate_category" >
        <ListPreference
            android:entries="@array/safe_class_entries_for_milan_series"
            android:entryValues="@array/safe_class_entry_values_for_milan_series"
            android:key="key_safe_class"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_safe_class" />
        <ListPreference
            android:entries="@array/authenticate_fail_retry_count"
            android:entryValues="@array/authenticate_fail_retry_count"
            android:key="key_screen_on_authenticate_fail_retry_count"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_screen_on_authenticate_fail_retry_count" />
        <ListPreference
            android:entries="@array/authenticate_fail_retry_count"
            android:entryValues="@array/authenticate_fail_retry_count"
            android:key="key_screen_off_authenticate_fail_retry_count"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_screen_off_authenticate_fail_retry_count" />
        <ListPreference
            android:entries="@array/valid_touch_frame_threshold"
            android:entryValues="@array/valid_touch_frame_threshold"
            android:key="key_screen_on_valid_touch_frame_threshold"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_screen_on_valid_touch_frame_threshold" />
        <ListPreference
            android:entries="@array/valid_touch_frame_threshold"
            android:entryValues="@array/valid_touch_frame_threshold"
            android:key="key_screen_off_valid_touch_frame_threshold"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_screen_off_valid_touch_frame_threshold" />
        <ListPreference
            android:entries="@array/image_quality_threshold_for_mistake_touch"
            android:entryValues="@array/image_quality_threshold_for_mistake_touch"
            android:key="key_image_quality_threshold_for_mistake_touch"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_image_quality_threshold_for_mistake_touch" />
        <ListPreference
            android:entries="@array/authenticate_order_entries"
            android:entryValues="@array/authenticate_order_entry_values"
            android:key="key_authenticate_order"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_authenticate_order" />
    </PreferenceCategory>

    <PreferenceCategory
