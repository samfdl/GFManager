        <SwitchPreference
            android:key="key_require_down_and_up_in_pairs_for_nav_mode"
            android:summary="@string/summary_require_down_and_up_in_pairs_for_nav_mode"
            android:title="@string/title_require_down_and_up_in_pairs_for_nav_mode" />
        <SwitchPreference
            android:key="key_support_set_spi_speed_in_tee"
            android:summary="@string/summary_support_set_spi_speed_in_tee"
            android:title="@string/title_support_set_spi_speed_in_tee" />
        <ListPreference
            android:entries="@array/template_update_save_threshold"
