        android:layout="@layout/preference_category"
        android:title="@string/badpoint_category">
        <EditTextPreference
            android:key="key_bad_point_avg_diff_val_threshold"
            android:title="@string/avg_diff_val"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_local_small_bad_pixel_threshold"
            android:title="@string/local_small_bad_pixel"
            android:numeric="integer"/>

        <EditTextPreference
            android:key="key_bad_point_local_big_bad_pixel_threshold"
            android:title="@string/local_big_bad_pixel"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_total_bad_pixel_num_threshold"
            android:title="@string/total_bad_pixel"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_local_bad_pixel_num_threshold"
            android:title="@string/local_bad_pixel_num"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_local_worst_num_threshold"
            android:title="@string/local_worst"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_total_gradient"
            android:title="@string/total_bad_gradient"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_local_gradient"
            android:title="@string/local_bad_gradient"
            android:numeric="integer"
            />

        <EditTextPreference
            android:key="key_bad_point_local_worst_gradient"
            android:title="@string/local_worst_gradient"
            android:numeric="integer"
            />

        <ListPreference
            android:entries="@array/bad_point_test_max_frame_number"
            android:entryValues="@array/bad_point_test_max_frame_number"
            android:key="key_bad_point_test_max_frame_number"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_bad_point_test_max_frame_number" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="dump"
        android:layout="@layout/preference_category"
        android:title="@string/dump_category" >
        <PreferenceScreen
            android:key="key_dump_config"
            android:summary="@string/summary_dump"
            android:title="@string/dump_functional_test">
            <intent
                android:targetClass="com.goodix.gftest.DumpFunctionalTestActivity"
                android:targetPackage="com.goodix.gftest" />
        </PreferenceScreen>
    </PreferenceCategory>

    <PreferenceCategory
        android:key="swipe_enroll"
        android:layout="@layout/preference_category"
        android:title="@string/swipe_enroll_category" >
        <SwitchPreference
            android:key="key_swipe_enroll_switch"
            android:summary="@string/summary_swipe_enroll_switch"
            android:title="@string/title_swipe_enroll_switch" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="other"
        android:layout="@layout/preference_category"
        android:title="@string/other_category" >
        <SwitchPreference
            android:key="key_reissue_key_down_when_entry_ff_mode"
