            android:summary="@string/summary_support_sensor_broken_check"
            android:title="@string/title_support_sensor_broken_check" />

        <ListPreference
            android:dependency="key_support_sensor_broken_check"
            android:entries="@array/array_broken_check_disable_sensor"
            android:entryValues="@array/array_broken_check_disable_sensor"
            android:key="key_broken_pixel_threshold_for_disable_sensor"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_broken_pixel_threshold_for_disable_sensor" />
        <ListPreference
            android:dependency="key_support_sensor_broken_check"
            android:entries="@array/array_broken_check_disable_study"
            android:entryValues="@array/array_broken_check_disable_study"
            android:key="key_broken_pixel_threshold_for_disable_study"
            android:negativeButtonText="@string/canceled"
            android:title="@string/title_broken_pixel_threshold_for_disable_study" />

        <SwitchPreference
            android:key="key_report_key_event_only_enroll_authenticate"
            android:summary="@string/summary_report_key_event_only_enroll_authenticate"
            android:title="@string/title_report_key_event_only_enroll_authenticate" />
        <SwitchPreference
